{% extends 'base.html.twig' %}

{% block body %}
    <h1 class="mt-5 mb-5 text-center">Liste des utilisateurs</h1>
    <table class="table text-center">
        <thead>
            <tr>
                <th>Nom</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            {% for user in users %}
                <tr>
                    <td>{{ user.username }}</td>
                    <td>{{ user.email }}</td>
                </tr>
            {% endfor %}
        </tbody>
    </table>
{% endblock %}